<# import "header" #>

<# foreach post in posts #>
  <article>
    <header>
      <a href="<# post.link #>">
        <h2><# print post.title #></h2>
      </a>
      <span><# print post.publishedDate #></span>
    </header>
    <# print post.content #>
  </article>
<# endforeach #>

<# import "footer" #>